<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>loginsucessful</from-outcome>
            <to-view-id>/accueil.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/accueil.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>fromDashboardToConsultants</from-outcome>
            <to-view-id>/consultants.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>fromDashBoardToAjoutConsultant</from-outcome>
            <to-view-id>/ajoutConsultant.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>fromDashboardToDetails</from-outcome>
            <to-view-id>/detailsClient.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/consultants.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>fromConsultantsToDashboard</from-outcome>
            <to-view-id>/accueil.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>fromConsultantsToAjoutConsultant</from-outcome>
            <to-view-id>/ajoutConsultant.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/ajoutConsultant.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>fromAjoutConsultantToConsultants</from-outcome>
            <to-view-id>/consultants.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>fromAjoutConsultantToDashBoard</from-outcome>
            <to-view-id>/accueil.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/detailsClient.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>fromDetailsToDashboard</from-outcome>
            <to-view-id>/accueil.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
